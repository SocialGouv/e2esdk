{
  "name": "@e2esdk/crypto",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "description": "Cryptographic constructs & algorithms for e2eSDK client & server",
  "author": {
    "name": "Fran√ßois Best",
    "email": "npm.e2esdk@francoisbest.com",
    "url": "https://francoisbest.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./lite": {
      "require": "./dist/lite.cjs",
      "import": "./dist/lite.js",
      "types": "./dist/lite.d.ts"
    }
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/lite.ts"
    ],
    "treeshake": true
  },
  "scripts": {
    "dev": "tsup --format esm --dts --watch",
    "build:npm": "tsup --clean --format esm,cjs --dts",
    "build:server": "tsup --clean --format esm",
    "typecheck": "tsc",
    "test": "jest --color"
  },
  "dependencies": {
    "@stablelib/blake2b": "^1.0.1",
    "@stablelib/hex": "^1.0.1",
    "libsodium-wrappers": "^0.7.10",
    "tweetnacl": "^1.0.3",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/libsodium-wrappers": "^0.7.10",
    "@types/node": "^18.11.9",
    "jest": "^29.3.1",
    "jest-config": "workspace:*",
    "tsconfig": "workspace:*",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  }
}
