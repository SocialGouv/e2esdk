{
  "name": "@e2esdk/client",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "description": "End-to-end encryption client",
  "author": {
    "name": "Fran√ßois Best",
    "email": "npm.e2esdk@francoisbest.com",
    "url": "https://francoisbest.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "dts": true,
    "treeshake": true
  },
  "scripts": {
    "dev": "tsup --format esm --watch",
    "build:npm": "tsup --clean --format esm,cjs",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@e2esdk/api": "workspace:*",
    "@e2esdk/core": "workspace:*",
    "@e2esdk/crypto": "workspace:*",
    "local-state-sync": "1.0.0-beta.6",
    "mitt": "^3.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "tsconfig": "workspace:*",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  }
}
