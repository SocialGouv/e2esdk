{
  "name": "@e2esdk/core",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "description": "Shared code between e2eSDK client & server",
  "author": {
    "name": "Fran√ßois Best",
    "email": "npm.e2esdk@francoisbest.com",
    "url": "https://francoisbest.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "treeshake": true
  },
  "scripts": {
    "dev": "tsup --format esm --dts --watch",
    "build:npm": "tsup --clean --format esm,cjs --dts",
    "build:server": "tsup --clean --format esm",
    "typecheck": "tsc",
    "test": "jest --color"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "jest": "^29.3.1",
    "jest-config": "workspace:*",
    "tsconfig": "workspace:*",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  }
}
