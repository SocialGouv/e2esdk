{
  "name": "@e2esdk/devtools",
  "version": "0.0.0-semantically-released",
  "license": "Apache-2.0",
  "description": "Developer tools panel to inspect and interact with an e2esdk client",
  "author": {
    "name": "Fran√ßois Best",
    "email": "npm.e2esdk@francoisbest.com",
    "url": "https://francoisbest.com"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://localhost:4873"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "tsup": {
    "entry": [
      "src/index.tsx"
    ],
    "format": [],
    "dts": true,
    "treeshake": true
  },
  "scripts": {
    "dev": "vite --port 3000 --clearScreen false",
    "build:npm": "run-p build:npm:*",
    "build:npm:bundle": "vite build",
    "build:npm:types": "tsup",
    "typecheck": "tsc"
  },
  "peerDependencies": {
    "@e2esdk/client": "workspace:^"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.2",
    "@e2esdk/react": "workspace:^",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@tanstack/react-query": "^4.18.0",
    "@tanstack/react-query-devtools": "^4.18.0",
    "framer-motion": "^7.6.15",
    "react-focus-lock": "^2.9.2",
    "react-hook-form": "^7.40.0",
    "react-icons": "^4.7.1",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@e2esdk/client": "workspace:^",
    "@e2esdk/crypto": "workspace:^",
    "@e2esdk/server": "workspace:^",
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "dotenv": "^16.0.3",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-visualizer": "^5.8.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
