{
  "name": "@socialgouv/e2esdk-keygen",
  "version": "0.0.0-semantically-released",
  "license": "Apache-2.0",
  "description": "Cryptographic key generator for e2eSDK",
  "author": {
    "name": "Fran√ßois Best",
    "email": "npm.e2esdk@francoisbest.com",
    "url": "https://francoisbest.com"
  },
  "repository": "https://github.com/SocialGouv/e2esdk",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "sceau.json"
  ],
  "bin": {
    "keygen": "dist/keygen.js"
  },
  "type": "module",
  "sideEffects": false,
  "main": "dist/keygen.js",
  "tsup": {
    "entry": [
      "src/keygen.ts"
    ],
    "format": [
      "esm"
    ],
    "dts": true,
    "treeshake": true
  },
  "scripts": {
    "dev": "tsup --watch",
    "build:npm": "tsup --clean",
    "typecheck": "tsc",
    "keygen": "zx ./dist/keygen.js",
    "prepack": "sceau sign"
  },
  "dependencies": {
    "@socialgouv/e2esdk-crypto": "workspace:^",
    "@47ng/opaque-server": "^2.1.4",
    "zod": "^3.20.2",
    "zx": "^7.1.1"
  },
  "devDependencies": {
    "@socialgouv/e2esdk-config-typescript": "workspace:*",
    "sceau": "^1.3.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
