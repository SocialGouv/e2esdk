{
  "name": "@socialgouv/e2esdk-semantic-release",
  "version": "0.0.0-internal",
  "license": "Apache-2.0",
  "private": true,
  "description": "Release automation plugin for semantic-release",
  "author": {
    "name": "Fran√ßois Best",
    "email": "npm.e2esdk@francoisbest.com",
    "url": "https://francoisbest.com"
  },
  "repository": "https://github.com/SocialGouv/e2esdk",
  "files": [
    "dist/"
  ],
  "type": "commonjs",
  "sideEffects": false,
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js"
    }
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs"
    ],
    "dts": false,
    "treeshake": true
  },
  "scripts": {
    "dev": "tsup --watch",
    "build:npm": "tsup --clean",
    "typecheck": "tsc"
  },
  "dependencies": {
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@socialgouv/e2esdk-config-typescript": "workspace:*",
    "@types/node": "^18.15.5",
    "tsup": "^7.0.0",
    "typescript": "^4.9.5"
  }
}
