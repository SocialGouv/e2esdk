{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build:npm": {
      "dependsOn": ["^build:npm"],
      "outputs": ["dist/**", "build/**", ".vite/**"]
    },
    "build:server": {
      "dependsOn": ["^build:server"],
      "outputs": ["dist/**", "build/**"]
    },
    "typecheck": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
